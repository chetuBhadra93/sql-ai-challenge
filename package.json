{
  "name": "sql-ai-challenge",
  "version": "1.0.0",
  "description": "Natural Language to SQL application using NestJS, PostgreSQL, LangChain, and OpenAI",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p apps/api/tsconfig.json",
    "build:api": "tsc -p apps/api/tsconfig.json",
    "start": "node dist/apps/api/main.js",
    "start:dev": "ts-node-dev apps/api/main.ts",
    "start:dev:docker": "npm run docker:db && npm run start:dev",
    "docker:db": "docker-compose -f docker-compose.postgres.yml up -d",
    "docker:db:stop": "docker-compose -f docker-compose.postgres.yml down",
    "docker:db:logs": "docker-compose -f docker-compose.postgres.yml logs postgres",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs",
    "docker:psql": "docker exec -it sql-ai-postgres psql -U sql_ai_user -d sql_ai_db",
    "test": "jest",
    "test:e2e": "jest --config test/jest-e2e.json",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\""
  },
  "dependencies": {
    "@langchain/core": "^0.1.63",
    "@langchain/openai": "^0.0.14",
    "@nestjs/common": "^10.3.3",
    "@nestjs/core": "^10.3.3",
    "@nestjs/platform-express": "^10.3.3",
    "@types/pg": "^8.15.5",
    "dotenv": "^16.4.5",
    "langchain": "^0.1.25",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.25",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "nestjs",
    "natural-language",
    "sql",
    "postgresql",
    "langchain",
    "openai"
  ],
  "author": "",
  "license": "MIT"
}
